// ============================================================================
// TRANSFORM
// ============================================================================

/// 2D transform matrisi (3x3 affine transform)
pub tip Transform2D = yapı yap
    // | a  c  e |
    // | b  d  f |
    // | 0  0  1 |
    a: ondalık, b: ondalık,
    c: ondalık, d: ondalık,
    e: ondalık, f: ondalık
son

impl Transform2D:
    /// Birim matris (identity)
    pub fn identity() döndür Transform2D:
        döndür Transform2D yap
            a: 1.0, b: 0.0,
            c: 0.0, d: 1.0,
            e: 0.0, f: 0.0
        son
    son
    
    /// Öteleme (translate)
    pub fn translate(x: ondalık, y: ondalık) döndür Transform2D:
        döndür Transform2D yap
            a: 1.0, b: 0.0,
            c: 0.0, d: 1.0,
            e: x, f: y
        son
    son
    
    /// Ölçekleme (scale)
    pub fn scale(sx: ondalık, sy: ondalık) döndür Transform2D:
        döndür Transform2D yap
            a: sx, b: 0.0,
            c: 0.0, d: sy,
            e: 0.0, f: 0.0
        son
    son
    
    /// Döndürme (rotate)
    pub fn rotate(angle: ondalık) döndür Transform2D:
        değişken cos_a = cos(angle)
        değişken sin_a = sin(angle)
        döndür Transform2D yap
            a: cos_a, b: sin_a,
            c: -sin_a, d: cos_a,
            e: 0.0, f: 0.0
        son
    son
    
    /// Eğme (skew)
    pub fn skew(angle_x: ondalık, angle_y: ondalık) döndür Transform2D:
        döndür Transform2D yap
            a: 1.0, b: tan(angle_y),
            c: tan(angle_x), d: 1.0,
            e: 0.0, f: 0.0
        son
    son
    
    /// Matris çarpımı
    pub fn multiply(self, other: Transform2D) döndür Transform2D:
        döndür Transform2D yap
            a: self.a * other.a + self.c * other.b,
            b: self.b * other.a + self.d * other.b,
            c: self.a * other.c + self.c * other.d,
            d: self.b * other.c + self.d * other.d,
            e: self.a * other.e + self.c * other.f + self.e,
            f: self.b * other.e + self.d * other.f + self.f
        son
    son
    
    /// Nokta dönüşümü
    pub fn transform_point(self, x: ondalık, y: ondalık) döndür (ondalık, ondalık):
        döndür (
            self.a * x + self.c * y + self.e,
            self.b * x + self.d * y + self.f
        )
    son
    
    /// Ters matris
    pub fn invert(self) döndür Seçenek[Transform2D]:
        değişken det = self.a * self.d - self.b * self.c
        eğer det == 0.0:
            döndür boş
        son
        
        değişken inv_det = 1.0 / det
        döndür Bazı(Transform2D yap
            a: self.d * inv_det,
            b: -self.b * inv_det,
            c: -self.c * inv_det,
            d: self.a * inv_det,
            e: (self.c * self.f - self.d * self.e) * inv_det,
            f: (self.b * self.e - self.a * self.f) * inv_det
        son)
    son
son

// ============================================================================
// PATH
// ============================================================================

/// Vektör yolu (path)
pub tip Path = yapı yap
    commands: liste[PathCommand]
son

/// Yol komutları
pub tip PathCommand = enum yap
    MoveTo(x: ondalık, y: ondalık),
    LineTo(x: ondalık, y: ondalık),
    QuadraticCurveTo(cpx: ondalık, cpy: ondalık, x: ondalık, y: ondalık),
    BezierCurveTo(cp1x: ondalık, cp1y: ondalık, cp2x: ondalık, cp2y: ondalık, x: ondalık, y: ondalık),
    ArcTo(x1: ondalık, y1: ondalık, x2: ondalık, y2: ondalık, radius: ondalık),
    Arc(cx: ondalık, cy: ondalık, radius: ondalık, start_angle: ondalık, end_angle: ondalık, counter_clockwise: mantıksal),
    Ellipse(cx: ondalık, cy: ondalık, rx: ondalık, ry: ondalık, rotation: ondalık, start_angle: ondalık, end_angle: ondalık, counter_clockwise: mantıksal),
    Rect(x: ondalık, y: ondalık, width: ondalık, height: ondalık),
    RoundRect(x: ondalık, y: ondalık, width: ondalık, height: ondalık, radii: liste[ondalık]),
    ClosePath
son

impl Path:
    /// Yeni boş yol
    pub fn new() döndür Path:
        döndür Path yap commands: [] son
    son
    
    /// Kaleme taşı
    pub fn move_to(mut self, x: ondalık, y: ondalık) döndür Path:
        self.commands.push(PathCommand::MoveTo(x, y))
        döndür self
    son
    
    /// Çizgi çiz
    pub fn line_to(mut self, x: ondalık, y: ondalık) döndür Path:
        self.commands.push(PathCommand::LineTo(x, y))
        döndür self
    son
    
    /// Kuadratik Bezier eğrisi
    pub fn quadratic_curve_to(mut self, cpx: ondalık, cpy: ondalık, x: ondalık, y: ondalık) döndür Path:
        self.commands.push(PathCommand::QuadraticCurveTo(cpx, cpy, x, y))
        döndür self
    son
    
    /// Kübik Bezier eğrisi
    pub fn bezier_curve_to(mut self, cp1x: ondalık, cp1y: ondalık, cp2x: ondalık, cp2y: ondalık, x: ondalık, y: ondalık) döndür Path:
        self.commands.push(PathCommand::BezierCurveTo(cp1x, cp1y, cp2x, cp2y, x, y))
        döndür self
    son
    
    /// Yay (arc)
    pub fn arc(mut self, cx: ondalık, cy: ondalık, radius: ondalık, start_angle: ondalık, end_angle: ondalık) döndür Path:
        self.commands.push(PathCommand::Arc(cx, cy, radius, start_angle, end_angle, yanlış))
        döndür self
    son
    
    /// Elips
    pub fn ellipse(mut self, cx: ondalık, cy: ondalık, rx: ondalık, ry: ondalık, rotation: ondalık, start_angle: ondalık, end_angle: ondalık) döndür Path:
        self.commands.push(PathCommand::Ellipse(cx, cy, rx, ry, rotation, start_angle, end_angle, yanlış))
        döndür self
    son
    
    /// Dikdörtgen
    pub fn rect(mut self, x: ondalık, y: ondalık, width: ondalık, height: ondalık) döndür Path:
        self.commands.push(PathCommand::Rect(x, y, width, height))
        döndür self
    son
    
    /// Yuvarlak köşeli dikdörtgen
    pub fn round_rect(mut self, x: ondalık, y: ondalık, width: ondalık, height: ondalık, radius: ondalık) döndür Path:
        self.commands.push(PathCommand::RoundRect(x, y, width, height, [radius]))
        döndür self
    son
    
    /// Yolu kapat
    pub fn close(mut self) döndür Path:
        self.commands.push(PathCommand::ClosePath)
        döndür self
    son
    
    /// Daire oluştur
    pub fn circle(x: ondalık, y: ondalık, radius: ondalık) döndür Path:
        döndür Path::new()
            .arc(x, y, radius, 0.0, 6.28318530718)  // 2π
            .close()
    son
son

