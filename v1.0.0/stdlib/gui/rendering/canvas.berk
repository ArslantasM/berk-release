impl Font:
    pub fn default() döndür Font:
        döndür Font yap
            family: "sans-serif",
            size: 14.0,
            weight: FontWeight::Normal,
            style: FontStyle::Normal,
            variant: FontVariant::Normal
        son
    son
    
    pub fn new(family: yazı, size: ondalık) döndür Font:
        değişken f = Font::default()
        f.family = family
        f.size = size
        döndür f
    son
    
    pub fn bold(mut self) döndür Font:
        self.weight = FontWeight::Bold
        döndür self
    son
    
    pub fn italic(mut self) döndür Font:
        self.style = FontStyle::Italic
        döndür self
    son
son

