/// Dolgu stili
pub tip FillStyle = enum yap
    /// Düz renk
    Solid(color: Color),
    
    /// Doğrusal gradient
    LinearGradient(gradient: LinearGradient),
    
    /// Radyal gradient
    RadialGradient(gradient: RadialGradient),
    
    /// Konik gradient
    ConicGradient(gradient: ConicGradient),
    
    /// Desen (pattern)
    Pattern(pattern: Pattern)
son

/// Çizgi stili
pub tip StrokeStyle = enum yap
    Solid(color: Color),
    LinearGradient(gradient: LinearGradient),
    RadialGradient(gradient: RadialGradient)
son

/// Doğrusal gradient
pub tip LinearGradient = yapı yap
    x0: ondalık, y0: ondalık,  // Başlangıç noktası
    x1: ondalık, y1: ondalık,  // Bitiş noktası
    stops: liste[ColorStop]
son

/// Radyal gradient
pub tip RadialGradient = yapı yap
    x0: ondalık, y0: ondalık, r0: ondalık,  // İç daire
    x1: ondalık, y1: ondalık, r1: ondalık,  // Dış daire
    stops: liste[ColorStop]
son

/// Konik gradient
pub tip ConicGradient = yapı yap
    cx: ondalık, cy: ondalık,  // Merkez
    start_angle: ondalık,       // Başlangıç açısı (radyan)
    stops: liste[ColorStop]
son

/// Renk durağı
pub tip ColorStop = yapı yap
    offset: ondalık,  // 0.0 - 1.0
    color: Color
son

/// Desen (pattern)
pub tip Pattern = yapı yap
    image: ImageData,
    repetition: PatternRepetition
son

pub tip PatternRepetition = enum yap
    Repeat,
    RepeatX,
    RepeatY,
    NoRepeat
son

