// ============================================================================
// IMAGE DATA
// ============================================================================

/// Görüntü verisi
pub tip ImageData = yapı yap
    width: tamsayı,
    height: tamsayı,
    data: liste[tamsayı]  // RGBA piksel verileri
son

impl ImageData:
    pub fn new(width: tamsayı, height: tamsayı) döndür ImageData:
        değişken size = width * height * 4
        döndür ImageData yap
            width: width,
            height: height,
            data: liste[tamsayı](size, 0)
        son
    son
    
    /// Piksel oku
    pub fn get_pixel(self, x: tamsayı, y: tamsayı) döndür Color:
        değişken idx = (y * self.width + x) * 4
        döndür Color yap
            r: self.data[idx],
            g: self.data[idx + 1],
            b: self.data[idx + 2],
            a: self.data[idx + 3]
        son
    son
    
    /// Piksel yaz
    pub fn set_pixel(mut self, x: tamsayı, y: tamsayı, color: Color) döndür void:
        değişken idx = (y * self.width + x) * 4
        self.data[idx] = color.r
        self.data[idx + 1] = color.g
        self.data[idx + 2] = color.b
        self.data[idx + 3] = color.a
    son
son

// ============================================================================
// CANVAS IMPLEMENTATION
// ============================================================================

impl Canvas:
    /// Yeni canvas oluştur
    pub fn new(width: tamsayı, height: tamsayı) döndür Canvas:
        döndür Canvas yap
            width: width,
            height: height,
            state_stack: [],
            current_state: CanvasState::default(),
            native_handle: __native_create_canvas(width, height)
        son
    son
    
    /// Boyutlandır
    pub fn resize(mut self, width: tamsayı, height: tamsayı) döndür void:
        self.width = width
        self.height = height
        __native_resize_canvas(self.native_handle, width, height)
    son
    
    // === STATE MANAGEMENT ===
    
    /// Durumu kaydet
    pub fn save(mut self) döndür void:
        self.state_stack.push(self.current_state.clone())
        __native_canvas_save(self.native_handle)
    son
    
    /// Durumu geri yükle
    pub fn restore(mut self) döndür void:
        eğer değişken state = self.state_stack.pop():
            self.current_state = state
            __native_canvas_restore(self.native_handle)
        son
    son
    
    /// Durumu sıfırla
    pub fn reset(mut self) döndür void:
        self.current_state = CanvasState::default()
        self.state_stack.clear()
        __native_canvas_reset(self.native_handle)
    son
    
    // === TRANSFORM ===
    
    /// Öteleme
    pub fn translate(mut self, x: ondalık, y: ondalık) döndür void:
        self.current_state.transform = self.current_state.transform
            .multiply(Transform2D::translate(x, y))
        __native_canvas_translate(self.native_handle, x, y)
    son
    
    /// Döndürme
    pub fn rotate(mut self, angle: ondalık) döndür void:
        self.current_state.transform = self.current_state.transform
            .multiply(Transform2D::rotate(angle))
        __native_canvas_rotate(self.native_handle, angle)
    son
    
    /// Ölçekleme
    pub fn scale(mut self, sx: ondalık, sy: ondalık) döndür void:
        self.current_state.transform = self.current_state.transform
            .multiply(Transform2D::scale(sx, sy))
        __native_canvas_scale(self.native_handle, sx, sy)
    son
    
    /// Transform matrisi ayarla
    pub fn set_transform(mut self, transform: Transform2D) döndür void:
        self.current_state.transform = transform
        __native_canvas_set_transform(self.native_handle,
            transform.a, transform.b, transform.c,
            transform.d, transform.e, transform.f)
    son
    
    /// Transform sıfırla
    pub fn reset_transform(mut self) döndür void:
        self.current_state.transform = Transform2D::identity()
        __native_canvas_reset_transform(self.native_handle)
    son
    
    // === FILL & STROKE STYLE ===
    
    /// Dolgu rengi ayarla
    pub fn set_fill_color(mut self, color: Color) döndür void:
        self.current_state.fill_style = FillStyle::Solid(color)
        __native_canvas_set_fill_color(self.native_handle, color.r, color.g, color.b, color.a)
    son
    
    /// Çizgi rengi ayarla
    pub fn set_stroke_color(mut self, color: Color) döndür void:
        self.current_state.stroke_style = StrokeStyle::Solid(color)
        __native_canvas_set_stroke_color(self.native_handle, color.r, color.g, color.b, color.a)
    son
    
    /// Çizgi genişliği ayarla
    pub fn set_line_width(mut self, width: ondalık) döndür void:
        self.current_state.line_width = width
        __native_canvas_set_line_width(self.native_handle, width)
    son
    
    /// Global alpha ayarla
    pub fn set_global_alpha(mut self, alpha: ondalık) döndür void:
        self.current_state.global_alpha = alpha
        __native_canvas_set_global_alpha(self.native_handle, alpha)
    son
    
    // === DRAWING SHAPES ===
    
    /// Dikdörtgen doldur
    pub fn fill_rect(mut self, x: ondalık, y: ondalık, width: ondalık, height: ondalık) döndür void:
        __native_canvas_fill_rect(self.native_handle, x, y, width, height)
    son
    
    /// Dikdörtgen çizgi
    pub fn stroke_rect(mut self, x: ondalık, y: ondalık, width: ondalık, height: ondalık) döndür void:
        __native_canvas_stroke_rect(self.native_handle, x, y, width, height)
    son
    
    /// Dikdörtgen temizle
    pub fn clear_rect(mut self, x: ondalık, y: ondalık, width: ondalık, height: ondalık) döndür void:
        __native_canvas_clear_rect(self.native_handle, x, y, width, height)
    son
    
    /// Yolu doldur
    pub fn fill_path(mut self, path: Path) döndür void:
        __native_canvas_fill_path(self.native_handle, path.commands)
    son
    
    /// Yolu çiz
    pub fn stroke_path(mut self, path: Path) döndür void:
        __native_canvas_stroke_path(self.native_handle, path.commands)
    son
    
    /// Daire doldur
    pub fn fill_circle(mut self, cx: ondalık, cy: ondalık, radius: ondalık) döndür void:
        self.fill_path(Path::circle(cx, cy, radius))
    son
    
    /// Daire çiz
    pub fn stroke_circle(mut self, cx: ondalık, cy: ondalık, radius: ondalık) döndür void:
        self.stroke_path(Path::circle(cx, cy, radius))
    son
    
    /// Yuvarlak köşeli dikdörtgen doldur
    pub fn fill_round_rect(mut self, x: ondalık, y: ondalık, width: ondalık, height: ondalık, radius: ondalık) döndür void:
        değişken path = Path::new().round_rect(x, y, width, height, radius)
        self.fill_path(path)
    son
    
    // === TEXT ===
    
    /// Font ayarla
    pub fn set_font(mut self, font: Font) döndür void:
        self.current_state.font = font
        __native_canvas_set_font(self.native_handle, font.family, font.size, font.weight, font.style)
    son
    
    /// Metin doldur
    pub fn fill_text(mut self, text: yazı, x: ondalık, y: ondalık) döndür void:
        __native_canvas_fill_text(self.native_handle, text, x, y)
    son
    
    /// Metin çiz (stroke)
    pub fn stroke_text(mut self, text: yazı, x: ondalık, y: ondalık) döndür void:
        __native_canvas_stroke_text(self.native_handle, text, x, y)
    son
    
    /// Metin ölçümü
    pub fn measure_text(self, text: yazı) döndür TextMetrics:
        döndür __native_canvas_measure_text(self.native_handle, text)
    son
    
    // === IMAGE ===
    
    /// Görüntü çiz
    pub fn draw_image(mut self, image: ImageData, x: ondalık, y: ondalık) döndür void:
        __native_canvas_draw_image(self.native_handle, image, x, y)
    son
    
    /// Görüntü çiz (boyutlandırılmış)
    pub fn draw_image_scaled(mut self, image: ImageData, x: ondalık, y: ondalık, width: ondalık, height: ondalık) döndür void:
        __native_canvas_draw_image_scaled(self.native_handle, image, x, y, width, height)
    son
    
    // === CLIPPING ===
    
    /// Clip region ayarla
    pub fn clip(mut self, path: Path) döndür void:
        self.current_state.clip_region = Bazı(path)
        __native_canvas_clip(self.native_handle, path.commands)
    son
    
    // === SHADOW ===
    
    /// Gölge ayarla
    pub fn set_shadow(mut self, color: Color, blur: ondalık, offset_x: ondalık, offset_y: ondalık) döndür void:
        self.current_state.shadow = ShadowSettings::new(color, blur, offset_x, offset_y)
