pub tip Font = yapı yap
    family: yazı,            // "Arial", "Roboto", etc.
    size: ondalık,           // Piksel cinsinden
    weight: FontWeight,
    style: FontStyle,
    variant: FontVariant
son

pub tip FontWeight = enum yap
    Thin,       // 100
    ExtraLight, // 200
    Light,      // 300
    Normal,     // 400
    Medium,     // 500
    SemiBold,   // 600
    Bold,       // 700
    ExtraBold,  // 800
    Black       // 900
son

pub tip FontStyle = enum yap
    Normal,
    Italic,
    Oblique
son

pub tip FontVariant = enum yap
