// BERK GUI Framework - Canvas Module
// ============================================================================
// 2D çizim API'si - vektör grafikleri, şekiller, metin, görüntüler
// ============================================================================

modül gui::rendering::canvas

kullan gui::style::colors::Color
kullan gui::core::window::Point

// ============================================================================
// CANVAS
// ============================================================================

/// 2D çizim tuvali (canvas)
pub tip Canvas = yapı yap
    /// Boyutlar
    width: tamsayı,
    height: tamsayı,
    
    /// Durum yığını (state stack)
    state_stack: liste[CanvasState],
    
    /// Aktif durum
    current_state: CanvasState,
    
    /// Native handle
    native_handle: tamsayı
son

/// Canvas durumu (kaydedilebilir/geri yüklenebilir)
pub tip CanvasState = yapı yap
    /// Transform matrisi
    transform: Transform2D,
    
    /// Dolgu rengi/stili
    fill_style: FillStyle,
    
    /// Çizgi rengi/stili
    stroke_style: StrokeStyle,
    
    /// Çizgi genişliği
    line_width: ondalık,
    
    /// Çizgi ucu stili
    line_cap: LineCap,
    
    /// Çizgi birleşim stili
    line_join: LineJoin,
    
    /// Miter limit
    miter_limit: ondalık,
    
    /// Global alpha (şeffaflık)
    global_alpha: ondalık,
    
    /// Compositing mode
    composite_operation: CompositeOperation,
    
    /// Clip region
    clip_region: Seçenek[Path],
    
    /// Font
    font: Font,
    
    /// Gölge ayarları
    shadow: ShadowSettings
son

